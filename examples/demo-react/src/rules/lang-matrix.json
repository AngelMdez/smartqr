{
  "version": 1,
  "rules": [
    { "if": { "lang": ["es", "es-ES"] }, "target": { "web": "https://es.example.com" } },
    { "if": { "lang": ["en", "en-US", "en-GB"] }, "target": { "web": "https://en.example.com" } },
    { "if": { "lang": ["fr", "fr-FR"] }, "target": { "web": "https://fr.example.com" } },
    { "if": { "lang": ["de", "de-DE"] }, "target": { "web": "https://de.example.com" } }
  ],
  "default": { "target": { "web": "https://example.com" } }
}
