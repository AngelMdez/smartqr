{
  "version": 1,
  "rules": [
    { "if": { "dateRange": { "from": "2025-08-01T00:00:00.000Z", "to": "2025-08-31T23:59:59.999Z" } }, "target": { "web": "https://example.com/campaign/summer-2025" } },
    { "if": { "os": ["ios"] }, "target": { "deeplink": "myapp://home", "web": "https://example.com/ios" } },
    { "if": { "os": ["android"] }, "target": { "deeplink": "myapp://home", "web": "https://example.com/android" } },
    { "if": { "lang": ["es", "es-ES"] }, "target": { "web": "https://es.example.com" } },
    { "if": { "rollout": 30 }, "target": { "web": "https://example.com/landing-new" } }
  ],
  "default": { "target": { "web": "https://example.com" } }
}
