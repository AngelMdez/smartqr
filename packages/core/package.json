{
  "name": "@smartqr/core",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "//": "Build using tsup; adjust entry if needed",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "ua-parser-js": "^1.0.40",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@types/ua-parser-js": "^0.7.39",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4",
    "eslint": "^9.34.0"
  },
  "test": "vitest",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
