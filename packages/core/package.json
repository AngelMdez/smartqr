{
  "name": "@smartqr/core",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "ua-parser-js": "^1.0.40",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@types/ua-parser-js": "^0.7.39",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.1.9"
  },
  "test": "vitest",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
